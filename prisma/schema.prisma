generator client {
  provider   = "prisma-client-js"
  engineType = "binary"
}

datasource db {
  provider = "sqlite"
  url      = env("PRISMA_DATABASE_URL")
}

model User {
  id       String  @id
  email    String  @unique
  password String?
  //
  tokens   Token[]

  @@map("users")
}

model Token {
  id      String @id
  content String @unique
  //
  user    User   @relation(fields: [userId], references: [id])
  userId  String

  @@map("tokens")
}
